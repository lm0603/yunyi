.mui-content{
	min-height: 100vh;
}
.tm{
	border: none;	
}
.mui-radio input[type=radio][disabled]:before{
	opacity: 1;
}
.jg{
	margin:30px 3% 0 3% ;
}
.curret{
	color: rgb(254, 74, 72);
}
.wrong{
	color: #71c7d5;
}
.fx{
	
}
.fx>div:first-of-type{
	line-height: 2rem;
	margin-top: 20px;
	font-size: 1.2rem;
	padding: 10px 3%;
	border-bottom: 1px solid #ccc;
}
.fx>div:last-of-type{
	line-height: 1.6rem;
	padding: 10px 3%;
	font-size: 14px;
}
/*底部的区域*/
.fxBottom{
	width: 100%;
	position: fixed;
	bottom: 0;
	left: 0;
	border-top: 1px solid #eee;
	background-color: #fff;
}
.fxAll{
	height: 60px;
	line-height: 60px;
	/*padding-left: 30%;*/
	color: #bbb;
}
.fxAll>li{
	float: right;
	width: 20%;
}

.fxAll>li:first-of-type{
	float: right;
	width: 30%;
}
.fxAll>li .myIcon{
	color: #bbb;
}
.dui{
	color: rgb(113, 199, 213);
}
.cuo{
	 color: rgb(254, 74, 72);
}
.num{
	color: #666;
}
.fxAll>li.pj{
	float: left;
	margin-left: 3%;
}
#tmGroup{
	display: none;
	overflow: hidden;
	min-width: 96%;
	padding: 10px 3% 20px;
	transition: all 10s linear;
	border-top:1px #ccc solid;
}

#tmGroup>li{
	font-size: 0.7rem;
	width: 1.5rem;
	height: 1.5rem;
	text-align: center;
	line-height: 1.5rem;
	margin: 3px;
	border-radius: 50%;
	float: left;
	color: white;
	/*border: 1px solid orangered;*/
	background-color: #71C7D5;
}
#tmGroup>li.wrong{
	background-color: #FE4A48;
}
#tmGroup>li.active{
	background-color: #ccc;
}