#search{
	height: 140px;
	width: 100%;
	background: #ccc url(../img/bgTop.png);
	background-size: cover;
	filter: blur(15);
	padding: 10px 0;
	font-size: 0.8rem;
	position: fixed;
	top: 0;left: 0;
}
.mui-content{
	padding-top: 150px;
}
.seachArea{
	width: 90%;
	line-height: 40px;
	padding: 0 2%;
	margin:auto;
	height: 40px;
	background-color: #fff;
	border-radius: 20px;
}
input[type=text]{
	width: 70%;
	height: 38px;
	border: none;
	outline: none;
	margin-bottom: 0;
	font-size: 0.8rem;
}
#goSearch{
	float: right;
	display: inline-block;
	width: 15%;
	height: 20px;
	margin: 10px 0;
	line-height: 20px;
	text-align: right;
	padding-right: 2%;
	border-left: 1px solid #eee;
	color: #26bba5;
}
#searchMenu{
	margin:20px 0;
	height: 50px;
	outline: 1px solid #f4f4f4;
	overflow: hidden;
}
#searchMenu>li{
	float: left;
	width: 25%;
	line-height: 50px;
	color: white;
	text-align: center;
	position: relative;
}

#searchMenu>li:after{
	content: "\25E2";
	position: absolute;
    bottom: -10px;
    right: 0;
    font-size: 30px;
}
.mengban{
	width: 100vw;
	height:calc(100vh - 140px) ;
	background-color: rgba(0,0,0,0.2);
	position: fixed;
	top: 140px;
	left: 0;
	z-index: 99;
	display: none;
}
.danwei{
	width: 90vw;
	height: 40vh;
	background-color: #fff;
	position: fixed;
	top: 120px;
	left: 5vw;
	z-index: 100;
	/*display: none;*/
}
.danwei>div{
	height: 100%;
	overflow-x: auto;
	float: left;
	font-size: 0.9rem;
}
.dwLeft{
	width: 25%;
	background-color: #FcFcFc;
}
.dwRight{
	width: 75%;
	background-color: #F7F7F7;
}
.danwei>div li{
	line-height: 20px;
	margin: 10px 0;
	border-bottom: 1px solid #eee;
	text-align: center;
}

.dwLeft li.active{
	border-left:5px solid #00B096;
	color: #00B096;
}
.dwRight li.active{
	color: #00B096;
}