			.liR {
				width: 55%;
				float: left;
				padding: 10px 5%;
			}
			
			.anniu {
				margin-top: 30px;
				text-align: center;
			}
			
			#ks {
				width: 80%;
				border-radius: 10px;
				background-color: rgb(113, 199, 213);
				color: white;
				height: 40px;
				margin-top: 15px;
			}
			
			#mengban {
				width: 100vw;
				height: 100vh;
				background-color: rgba(0, 0, 0, 0.2);
				position: fixed;
				top: 0;
				left: 0;
				z-index: 9;
				display: none;
			}
			
			#close {
				position: absolute;
				width: 20px;
				height: 20px;
				top: -10px;
				right: -10px;
				border-radius: 50%;
				background-color: white;
				color: #ccc;
				text-align: center;line-height: 20px;
			}