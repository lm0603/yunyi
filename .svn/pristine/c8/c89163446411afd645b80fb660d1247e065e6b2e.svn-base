#menu {
	overflow: hidden;
	position: fixed;
	top: 0;
	width: 100%;
	z-index: 10;
}

.proAll li {
	position: relative;
}

#menu>li {
	width: 50%;
	float: left;
	height: 50px;
	color: #fff;
	line-height: 40px;
	text-align: center;
	background-color: #71C7D5;
	position: relative;
}

#menu>li.menuActive {
	color: rgb(0, 176, 150);
}

#menu>li>span {
	width: 20%;
	height: 2px;
	background-color: rgb(0, 176, 150);
	display: inline-block;
	position: absolute;
	bottom: 4px;
	left: 40%;
}

#menu>li {
	width: 33.3333333%;
}

.proAll {
	padding: 0;
}

.content_point_ask {
	width: 40%;
}

.ringhtBtn {
	position: absolute;
	right: 3%;
	bottom: 5px;
	display: none;   /*隐藏按钮*/
}

.ringhtBtn>span {
	margin-top: 5rem;
	padding: 2px;
	border: 1px solid #71C7D5;
	border-radius: 5px;
	font-size: 0.8rem;
	color: #fe4a48;
}

.h3 {
	margin: 0;
}

.content_point {
	margin: 2px 0;
}


