.mui-title>input {
	width: 70%;
	margin: 0;
	padding: 0 2%;
	height: 30px;
	border-radius: 15px;
	font-size: 0.8rem;
}

header.mui-bar {
	height: 60px;
	box-shadow: none;
}

header.mui-bar>a.mui-action-back {
	color: white;
	padding-top: 20px;
}

.mui-bar .mui-title {
	margin-top: 10px;
}

#menu {
	overflow: hidden;
	margin-top: 16px;
	position: fixed;
	top: 0;
	width: 100%;
}

#menu>li {
	width: 50%;
	float: left;
	height: 50px;
	color: #fff;
	line-height: 40px;
	text-align: center;
	background-color: #71C7D5;
	position: relative;
}

#menu>li.menuActive {
	color: rgb(0, 176, 150);
}

#menu>li>span {
	width: 20%;
	height: 2px;
	background-color: rgb(0, 176, 150);
	display: inline-block;
	position: absolute;
	bottom: 4px;
	left: 40%;
}

header.mui-bar {
	height: 44px;
}

header.mui-bar .mui-title {
	margin: 0;
}

#menu {
	margin: 0;
}

.proAll {
	padding: 10px 0;
	margin-top: 50px;
	background-color: #f7f7f7;
}

.tx,
.tz {
	padding: 0 3%;
}

.tx>li {
	padding: 10px 0;
	border-bottom: 1px solid #999999;
}

.ck {
	float: right;
}

.txt,
.txt span {
	font-size: 12px;
}

.blue {
	color: rgb(0, 147, 167);
}

.tit {
	font-size: 14px;
	margin: 10px 0;
}

.time {
	font-size: 12px;
}

.tz>li {
	padding: 10px 0;
	overflow: hidden;
}

.img {
	width: 60px;
	height: 60px;
	border-radius: 50%;
	overflow: hidden;
	float: left;
}

.img img {
	width: 100%;
	min-height: 100%;
}

.tzxx {
	margin-left: 70px;
}

.tztit {
	line-height: 2rem;
}

.tzt {
	font-size: 12px;
	color: #8F8F94;
	line-height: 2rem;
}

.new {
	color: white;
	font-size: 12px;
	padding: 0 10px;
	background-color: #FE4A48;
	border-radius: 5px;
}